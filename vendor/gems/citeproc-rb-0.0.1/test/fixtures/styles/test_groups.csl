<?xml version="1.0" encoding="UTF-8"?>
<?oxygen RNGSchema="http://xbiblio.svn.sourceforge.net/viewvc/*checkout*/xbiblio/csl/schema/trunk/csl.rnc" type="compact"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" xml:lang="en">
  <info>
  </info>
  <macro name="foo">
     <text prefix="' " value="appear in the"/>
  </macro>
  <macro name="bar">
     <text prefix=" " value="citation" suffix="."/>
  </macro>
  <citation>
    <layout suffix=".">
    </layout>
  </citation>
  <bibliography>
    <layout suffix=".">
        <group>
         <text value="test"/>
         <text variable="URL"/>
         <text macro="foo"/>
         <text macro="bar"/>
        </group>
        <group>
         <text value="The words" suffix=" '"/>
         <text variable="title"/>
         <text macro="foo"/>
         <text macro="bar"/>
        </group>
    </layout>
  </bibliography>
</style>
