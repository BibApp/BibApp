%table#publishers
  %tr
    %th.span-12 #{t '.publisher_name'}
    %th.span-8= t('.recent_year')
    %th.span-1.last #{Publication.human_name_pl}
  - @current_objects.each do |publisher|
    %tr{:class => cycle("odd", "even")}
      %td
        = render 'shared/sherpa_icon', :publisher => publisher
        #{link_to publisher.name, publisher_path(publisher)}
      %td #{publisher.most_recent_year}
      %td #{publisher.publications.length}
